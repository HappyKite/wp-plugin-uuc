{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["react"],
    "env": {
        "amd": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
        "jsx": true
        }
    },
    "rules": {
        "no-console": 0
    },
    "globals": {
        "console": true,
        "module": true,
        "__dirname": true
    }
}